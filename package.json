{
  "name": "etcetera",
  "description": "read service config from etcd",
  "version": "2.3.3",
  "author": "C J Silverio <ceej@npmjs.com>",
  "bin": {
    "et": "bin/etcetera.js",
    "etcetera": "bin/etcetera.js",
    "upstarter": "bin/upstarter.js",
    "lambdaconf": "bin/lambdaconf.js"
  },
  "bugs": {
    "url": "https://github.com/npm/etcetera/issues"
  },
  "dependencies": {
    "@npmcorp/etcd-result-objectify": "^1.0.0",
    "@npmcorp/etcdjs": "^2.4.3",
    "async": "^3.1.0",
    "chalk": "^2.4.2",
    "lodash.map": "~4.6.0",
    "nunjucks": "^3.2.0",
    "rc": "~1.2.8",
    "shell-quote": "~1.6.1",
    "toml-require": "^1.2.0",
    "update-notifier": "~2.5.0",
    "yargs": "~11.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "eslint-config-ceejbot": "~1.1.2",
    "mocha": "^6.1.4",
    "must": "~0.13",
    "nyc": "~12.0.2",
    "sinon": "~6.0.1",
    "standard-version": "^6.0.1",
    "xo": "^0.24.0"
  },
  "directories": {
    "test": "test"
  },
  "homepage": "https://github.com/npm/etcetera#readme",
  "keywords": [
    "configuration",
    "etcd"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/etcetera.git"
  },
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "xo",
    "test": "nyc mocha -R spec test",
    "travis": "npm run lint && nyc mocha -R spec test",
    "release": "standard-version"
  },
  "xo": {
    "extends": "eslint-config-ceejbot",
    "ignores": [
      "public/**",
      "lib/controllers/**",
      "assets/**"
    ],
    "rules": {
      "unicorn/prefer-starts-ends-with": 0,
      "prefer-arrow-callback": 0,
      "prefer-destructuring": 0
    }
  }
}
